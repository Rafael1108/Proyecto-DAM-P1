  <ImageView
      android:id="@+id/imageView"
      android:layout_width="154dp"
      android:layout_height="230dp"
      android:layout_marginEnd="300dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView"
      app:srcCompat="@drawable/ic_atencion" />

  <ImageView
      android:id="@+id/imageView3"
      android:layout_width="142dp"
      android:layout_height="147dp"
      android:layout_marginStart="300dp"
      android:layout_marginTop="40dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView"
      app:srcCompat="@drawable/ic_aprendizaje" />

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="157dp"
      android:layout_height="236dp"
      android:layout_marginTop="50dp"
      android:layout_marginEnd="300dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView"
      app:srcCompat="@drawable/ic_percepcion" />

  <ImageView
      android:id="@+id/imageView5"
      android:layout_width="147dp"
      android:layout_height="156dp"
      android:layout_marginStart="300dp"
      android:layout_marginTop="140dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView3"
      app:srcCompat="@drawable/ic_pensamiento" />

  <TextView
      android:id="@+id/atencionTextView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="300dp"
      android:text="Atenci贸n"
      android:textSize="24sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView" />

  <TextView
      android:id="@+id/aprendizajeTextView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="300dp"
      android:layout_marginTop="45dp"
      android:text="Aprendizaje"
      android:textSize="24sp"
      app:layout_constraintEnd_toEndOf="parent"
